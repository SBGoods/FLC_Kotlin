<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid
                android:color="@color/darkerBlue" />
            <stroke
                android:width="3dp"
                android:color="@color/darkerBlue"
                />
            <size
                android:width="85dp"
                android:height="85dp"/>
            <padding android:top="4dp"
                android:right="4dp"
                android:bottom="4dp"
                android:left="4dp"/>
        </shape>
    </item>
</layer-list>